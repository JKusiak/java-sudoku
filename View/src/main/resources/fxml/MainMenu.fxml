<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" fx:controller="MenuController">
   <Button fx:id="playBtn" layoutX="556.0" layoutY="321.0" mnemonicParsing="false" onAction="#clickPlay"
           prefHeight="78.0" prefWidth="169.0" text="%playBtn">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Button>
   <ComboBox fx:id="difficultyBox" layoutX="538.0" layoutY="519.0" prefHeight="34.0" prefWidth="204.0">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </ComboBox>
   <ComboBox fx:id="languageBox" layoutX="1164.0" layoutY="14.0" prefHeight="25.0" prefWidth="102.0"
             promptText="%lang_box">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </ComboBox>
   <Label fx:id="authorsLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="1097.0" layoutY="672.0"
          prefHeight="34.0" prefWidth="169.0" text="-" textAlignment="CENTER"/>
</AnchorPane>
