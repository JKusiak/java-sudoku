          <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
      </rowConstraints>
    </GridPane>
    <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMenu"
            prefHeight="44.0" prefWidth="83.0" text="%backBtn" textAlignment="CENTER">
        <cursor>
          <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <Button fx:id="checkBtn" layoutX="1135.0" layoutY="598.0" mnemonicParsing="false" onAction="#checkGame"
            prefHeight="44.0" prefWidth="131.0" text="%checkBtn" textAlignment="CENTER">
